    "        learning_rate=0.3,\n",
    "        max_depth=8,\n",
    "        objective='regression',\n",
    "        verbose=-1  # Silent mode\n",
    "    )\n",
    "    \n",
    "    reg_lgb.fit(\n",
    "        X_train, y_train,\n",
    "        eval_set=[(X_val, y_val)],\n",
    "        eval_metric=r2_score_lgb,  # Using your pre-defined metric\n",
